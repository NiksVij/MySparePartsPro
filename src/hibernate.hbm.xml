<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
   <class name="entities.VehicleService" table="vehicle_service">
      <meta attribute="class-description">
         This class contains the employee detail.
      </meta>
      <id name="serv_no" type="string" column="serv_no">
         <!--<generator class="native"/>-->
      </id>
      <property name="reg_no" column="reg_no" type="string"/>
      <property name="owner_name" column="owner_name" type="string"/>
      <property name="make_model" column="make_model" type="string"/>
      <property name="purchase_date" column="purchase_date" type="date"/>
      <property name="odo_reading" column="odo_reading" type="string"/>
      <property name="paid" column="paid" type="string"/>
   </class>
</hibernate-mapping>
